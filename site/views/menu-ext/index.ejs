<body class="home page-template-default page page-id-2 woocommerce-no-js">

    <input type="hidden" id="pp_menu_layout" name="pp_menu_layout" value="classicmenu"/>
    <input type="hidden" id="pp_enable_right_click" name="pp_enable_right_click" value=""/>
    <input type="hidden" id="pp_enable_dragging" name="pp_enable_dragging" value=""/>
    <input type="hidden" id="pp_image_path" name="pp_image_path" value="images/"/>
    <input type="hidden" id="pp_homepage_url" name="pp_homepage_url" value="index.html"/>
    <input type="hidden" id="pp_ajax_search" name="pp_ajax_search" value="1"/>
    <input type="hidden" id="pp_fixed_menu" name="pp_fixed_menu" value="1"/>
    <input type="hidden" id="pp_topbar" name="pp_topbar" value="1"/>
    <input type="hidden" id="post_client_column" name="post_client_column" value="4"/>
    <input type="hidden" id="pp_back" name="pp_back" value="Back"/>
    <input type="hidden" id="pp_page_title_img_blur" name="pp_page_title_img_blur" value="1"/>
    <input type="hidden" id="tg_project_filterable_link" name="tg_project_filterable_link" value=""/>
    <input type="hidden" id="pp_reservation_start_time" name="pp_reservation_start_time" value="11:00"/>
    <input type="hidden" id="pp_reservation_end_time" name="pp_reservation_end_time" value="21:00"/>
    <input type="hidden" id="pp_reservation_time_step" name="pp_reservation_time_step" value="30"/>
    <input type="hidden" id="pp_reservation_date_format" name="pp_reservation_date_format" value="mm/dd/yy"/>
    <input type="hidden" id="pp_footer_style" name="pp_footer_style" value="4"/>


    <!-- Begin template wrapper -->
    <div id="wrapper" class="hasbg">
        <div class="header_style_wrapper">
            <!-- Begin top bar -->
            <div class="above_top_bar">
                <div class="page_content_wrapper">
                    <div class="social_wrapper">
                        <ul>
                            <li class="facebook"><a target="_blank" href="https://www.facebook.com/Siwabar-103598004901718"><i class="fab fa-facebook"></i></a></li>
                            <li class="instagram"><a target="_blank" title="Instagram" href="https://www.instagram.com/siwabar/"><i class="fab fa-instagram"></i></a></li>
                        </ul>
                    </div>
                    <div class="extend_top_contact_info top_contact_info">
                        <span id="top_contact_address"><i class="fa fa-map-marker"></i>Beruti 3336, Paseo del Sol | Palermo, Buenos Aires</span>
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div id="page_caption" class="hasbg parallax baseline ">
        <div class="parallax_overlay_header"></div>
        <div id="bg_regular" style="background-image:url(/images/menu-ext/banner1.jpg);"></div>


        <div class="page_title_wrapper baseline" data-stellar-ratio="1.3">
            <div class="page_title_inner baseline">
                <h1 class="withtopbar">Menú</h1>
            </div>
        </div>
    </div>

    <div class="ppb_wrapper hasbg withtopbar">

        <% data.forEach(function(category) { %>
        <div class="one" style="padding:50px 0 50px 0;">
            <div class="standard_wrapper">
                <div class="page_content_wrapper">
                    <div class="inner">
                        <div style="text-align:center;"><h3 class="ppb_menu_title"><%= category.name %></h3></div>
                        <% category.childsCategories.forEach(function(subCategory) { %>
                        <div class="menu_container">
                            <h2 class="ppb_title_first"><%= subCategory.name %></h2>
                            <div class="menu_items">
                                <% subCategory.foods.forEach(function(menuFood) { %>
                                <div class="one_half ">
                                    <div id="menu_3197" class="menu_content_classic">
                                        <h5 class="menu_post">
                                            <span class="menu_title"><%= menuFood.name %></span>
                                            <span class="menu_dots"></span>
                                            <span class="menu_price">$<%= menuFood.price %></span></h5>
                                        <div class="post_detail menu_excerpt"><%= menuFood.description %></div>
                                        <div class="menu_highlight"><i class="fa fa-star"></i></div>
                                    </div>
                                </div>
                                <% }); %>
                            </div>
                        </div>
                        <% }); %>
                    </div>
                </div>
            </div>
        </div>
        
        <% if (category.id == 1) { %>
        <div class="parallax title" data-image="/images/menu-ext/siwabar-comer-img-2021_sep.jpeg" data-width="1348" data-height="899" data-content-height="60">
            <div class="parallax_title">
                <h2 class="ppb_title"><span class="ppb_title_first" ></span></h2></div>
        </div>
        <% } %>
        <% }); %>
            
        <br class="clear"/>
        <div class="footer_bar_wrapper ">
            <div id="copyright">
                © Copyright Siwa Bar
            </div>
            <br class="clear"/>
            <a id="toTop"><i class="fas fa-arrow-up"></i></a>
        </div>
    <div id="overlay_background">
    </div>


    <script src='/javascripts/menu-ext/jquery.js'></script>
    <script src='/javascripts/menu-ext/jquery-migrate.min.js'></script>
    <script src='/javascripts/menu-ext/plugins/revslider/public/assets/js/jquery.themepunch.tools.min.js'></script>
