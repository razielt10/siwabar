    <script src='/javascripts/menu-ext/plugins/hw-parallax.js'></script>
    <script src='/javascripts/menu-ext/plugins/jquery.stellar.min.js'></script>
    <script src='/javascripts/menu-ext/plugins/jquery.resizeimagetoparent.min.js'></script>
    <script src='/javascripts/menu-ext/plugins/custom_plugins.js'></script>
    <script src='/javascripts/menu-ext/plugins/custom.js'></script>
    <script src='/javascripts/menu-ext/plugins/core.min.js'></script>
    <script src='/javascripts/menu-ext/plugins/datepicker.min.js'></script>
    <script src='/javascripts/menu-ext/plugins/custom-date.js'></script>
    <script src='/javascripts/menu-ext/plugins/jquery.timepicker.js'></script>
    <script src='/javascripts/menu-ext/plugins/custom-time.js'></script>
    <script src='/javascripts/menu-ext/plugins/jquery.validate.js'></script>
